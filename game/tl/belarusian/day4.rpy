translate belarusian date_main_marta_p2bbb_6f19209c:

    # p "Hey, Marta!"
    p "Марта!"

# game/day4.rpy:1516
translate belarusian date_main_marta_p2bbb_9f464dd4:

    # m "That's my name."
    m "Да, меня так зовут."

# game/day4.rpy:1518
translate belarusian date_main_marta_p2bbb_366eba21:

    # p "What's that expo about?"
    p "А что за выставка?"

# game/day4.rpy:1519
translate belarusian date_main_marta_p2bbb_aaacb8f1:

    # m "Does it matter?"
    m "Какая разница?"

# game/day4.rpy:1520
translate belarusian date_main_marta_p2bbb_cde92cc7:

    # p "No."
    p "Не-а."

# game/day4.rpy:1522
translate belarusian date_main_marta_p2bbb_09c3e7a6:

