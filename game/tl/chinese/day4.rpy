    # p "Three..."
    p "三……"

# game/day4.rpy:1567
translate chinese date_main_marta_p2bbb_7a063e82:

    # p "Four..."
    p "四……"

# game/day4.rpy:1568
translate chinese date_main_marta_p2bbb_e5b7b6ca:

    # "The room just got warmer for no reason."
    "突然，卧室觉得更热。"

# game/day4.rpy:1569
