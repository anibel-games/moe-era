    # e "No, just the stock images I got from the web!"
    e "いや、ウェブでとったストックフォトだけだよ！"

# game/added.rpy:1153
translate japanese ermy_wave_reaction_d64e691a:

    # s "It's nice to see you like Rodin."
    s "ロダンみたい"

# game/added.rpy:1161
translate japanese ermy_wave_reaction_c606e70a:

    # e "Who's the guy?"
    e "その人誰？"

# game/added.rpy:1168
